<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
		PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
		"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.cp.license.trans.dao.Apply_STMapper">
	<cache />
	<select id="findByName"
		resultType="com.cp.license.trans.model.Apply_ST">
		select a.* from (select AstId from license where LicenseName=#{name}) l left join apply_st a on l.AstId=a.Id
	</select>
</mapper>