<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
		PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
		"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.cp.license.trans.dao.LicenseMapper">
	<cache />
	<select id="findByName" resultType="com.cp.license.trans.model.License">
		select * from license
		where LicenseName=#{name}
	</select>
	
</mapper>